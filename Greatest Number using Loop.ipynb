     "output_type": "stream",
     "text": [
      "Enter numbers with whitespace in between .\n",
      "54 75 927 637 32 90\n",
      "The largest element is = 927\n"
     ]
    }
   ],
   "source": [
    "l=input(\"Enter numbers with whitespace in between .\\n\")\n",
    "l=l.split()\n",
    "a=list(map(int,l))\n",
    "a.sort()\n",
    "print(\"The largest element is =\", a[-1])"
